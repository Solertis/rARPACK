importClassesFrom("Matrix", dgeMatrix, dgCMatrix, dsyMatrix)
import(Rcpp)
import(RcppEigen)

S3method(eigs,matrix)
S3method(eigs,dgeMatrix)
S3method(eigs,dgCMatrix)
S3method(eigs,dsyMatrix)

S3method(svds,dgCMatrix)
S3method(svds,dsyMatrix)
S3method(svds,matrix)

export(eigs)
export(eigs_sym)
export(svds)
